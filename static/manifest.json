{
  "name": "Restaurant Ordering System",
  "short_name": "RestaurantApp",
  "description": "Multi-tenant restaurant ordering system with real-time order tracking",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#d63384",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["food", "business", "productivity"],
  "screenshots": [
    {
      "src": "/static/images/screenshot-mobile.png",
      "sizes": "360x640",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile view of the restaurant ordering interface"
    },
    {
      "src": "/static/images/screenshot-desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Desktop view of the restaurant management dashboard"
    }
  ],
  "icons": [
    {
      "src": "/static/images/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/static/images/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "View Menu",
      "short_name": "Menu",
      "description": "Browse restaurant menu items",
      "url": "/restaurant/menu/",
      "icons": [
        {
          "src": "/static/images/menu-icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Order History",
      "short_name": "Orders",
      "description": "View your order history",
      "url": "/orders/history/",
      "icons": [
        {
          "src": "/static/images/orders-icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Kitchen Dashboard",
      "short_name": "Kitchen",
      "description": "Manage orders and kitchen operations",
      "url": "/orders/kitchen/",
      "icons": [
        {
          "src": "/static/images/kitchen-icon.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+restaurant",
      "url": "/orders/%s"
    }
  ],
  "share_target": {
    "action": "/orders/share/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/orders/import/",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "/static/images/import-icon.png",
          "sizes": "192x192"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "offline_warning": "This app requires an internet connection for full functionality. Some features may be limited when offline.",
  "features": [
    "real-time-updates",
    "offline-support",
    "push-notifications",
    "background-sync",
    "multi-tenant"
  ]
}